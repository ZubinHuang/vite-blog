{
  "name": "@AaronWong/viteblog",
  "version": "1.0.2",
  "main": "index.js",
  "homepage": "https://github.com/ZubinHuang/vite-blog/",
  "author": "AaronWong",
  "engines": {
    "node": ">=14.18.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "version": "# 修改 version  \n zx bump.mjs",
    "start": "vitepress dev docs",
    "build": "# 打包 \n npm run docs:build",
    "docs:dev": "vitepress dev docs",
    "commit": "# 发送冲锋 \n npm run build && npm run version && npm run changelog && git add . &&  git cz ",
    "push": "# 发送冲锋 \n npm run build && npm run version && npm run changelog && git add . &&  git cz && git ps",
    "changelog": "# 拉取 changlog  \n conventional-changelog -p custom-config -i docs/CHANGELOG/index.md -s -r 0",
    "docs:build": "vitepress build docs",
    "prepare": "husky install",
    "docs:serve": "vitepress serve docs"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@commitlint/config-conventional": "^17.8.0",
    "@types/node": "^20.8.6",
    "commitizen": "^4.3.0",
    "commitlint": "^17.8.0",
    "commitlint-config-cz": "^0.13.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "flexsearch": "^0.7.31",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.1",
    "markdown-it": "^13.0.2",
    "prettier": "^3.0.3",
    "prompts": "^2.4.2",
    "vitepress": "^1.0.2",
    "vitepress-plugin-music": "^0.0.9",
    "vitepress-plugin-search": "^1.0.4-alpha.22",
    "zx": "^7.2.3"
  },
  "dependencies": {
    "@xiaomh/vue3-alan-vite-component": "1.2.3",
    "blueimp-md5": "^2.19.0",
    "dayjs": "^1.11.10",
    "fast-glob": "^3.3.1",
    "gray-matter": "^4.0.3",
    "shiki": "^0.14.5",
    "valine": "^1.5.1"
  }
}
